<h3>Current Users</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th class="centered">Email</th>
      <th>Role</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.employee.name, employee_path(user.employee) %></td>
        <td class="centered"><%= user.email %></td>
        <td><%= user.employee.role%></td>
      </tr>
    <% end %>
  </tbody>
</table>
